{
  "name": "karma--tap--boilerplate",
  "version": "1.0.1",
  "description": "Karma and TAP boilerplate test files",
  "keywords": [
    "karma",
    "karma-tap",
    "tape",
    "tap"
  ],
  "scripts": {
    "karma": "karma start",
    "testonly:jsdom": "cross-env JSDOM=true npm run karma",
    "testonly:chrome": "npm run karma",
    "testonly": "npm run testonly:chrome",
    "test": "npm run testonly",
    "removeme:test:tap-spec:": "babel-node ./spec/test_spec.js | tap-spec",
    "removeme:test:faucet": "babel-node ./spec/test_spec.js | faucet"
  },
  "devDependencies": {
    "babel-cli": "6.x.x",
    "babel-loader": "6.x.x",
    "babel-preset-es2015": "6.9.0",
    "babel-preset-react": "6.x.x",
    "babel-preset-stage-2": "6.x.x",
    "babel-plugin-transform-class-properties": "6.x.x",
    "karma": "1.x.x",
    "karma-chrome-launcher": "1.x.x",
    "karma-jsdom-launcher": "3.x.x",
    "karma-tap": "2.x.x",
    "karma-tap-pretty-reporter": "1.x.x",
    "karma-webpack": "webpack/karma-webpack",
    "tape": "4.x.x",
    "faucet": "*",
    "tap-spec": "*",
    "tap-min": "*",
    "tap-difflet": "*",
    "tap-diff": "*",
    "tap-notify": "*",
    "tap-summary": "*",
    "tap-markdown": "*",
    "webpack": "^2.1.0-beta",
    "cross-env": "*",
    "mkdirp": "*",
    "rimraf": "*"
  },
  "author": "bySabi Files",
  "license": "ISC"
}
